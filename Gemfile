source 'https://rubygems.org'
gem 'github-pages'
gem 'activesupport', '~> 6.0'

# Jekyll Plugins
group :jekyll_plugins do
  gem 'jekyll-algolia'
end

# Windows does not include zoneinfo files, so bundle the tzinfo-data gem
gem 'wdm', '>= 0.1.0'
gem 'tzinfo', '~> 1.2'
gem 'tzinfo-data', '~> 1.2', platforms: [:mingw, :mswin, :x64_mingw, :jruby]
